{
  "name": "madebysurface",
  "version": "0.0.1",
  "description": "Website for madebysurface.co.uk",
  "homepage": "https://www.madebysurface.co.uk",
  "main": "index.js",
  "scripts": {
    "start": "npm run development && npm run serve",
    "development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "watch": "npm run development -- --watch",
    "production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "serve": "cross-env ELEVENTY_ENV=development npx @11ty/eleventy --serve",
    "build": "npm run production &&"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/trovster/madebysurface.git"
  },
  "author": "Trevor Morris",
  "license": "ISC",
  "devDependencies": {
    "@11ty/eleventy": "^0.11.0",
    "cross-env": "^7.0.2",
    "laravel-mix": "^5.0.4",
    "sass": "^1.26.5",
    "sass-loader": "^8.0.2",
    "vue-template-compiler": "^2.6.11"
  }
}
